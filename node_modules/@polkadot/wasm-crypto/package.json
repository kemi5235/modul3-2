    "@polkadot/wasm-crypto-wasm": "^4.2.1"
  },
  "devDependencies": {
    "@polkadot/util": "^7.2.1",
    "@polkadot/x-randomvalues": "^7.2.1"
  },
  "peerDependencies": {
